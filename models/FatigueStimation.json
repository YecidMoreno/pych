{
    "plugins": {
        "path": [
            "./plugins/",
            "/home/inception/git/pych/release/x86_64-linux-gnu/plugins/",
            "/home/inception/git/pych_plugins/release/x86_64-linux-gnu/plugins/"
        ],
        "commIO": {
            "SerialIO": "SerialIO.so"
        },
        "deviceIO": {
            "ScienceMode3": "ScienceMode3.so",
            "Channel_sm3": "Channel_sm3.so",
            "LogPrint": "LogPrint.so",
            "SensorIO": "SensorIO.so"
        },
        "controlIO": {
            "Functional": "Functional.so"
        }
    },
    "commIO": {
        "serial0": {
            "type": "SerialIO",
            "config": {
                "iface": "/dev/ttyUSB0",
                "baudrate": 115200,
                "timeout_sec": 0,
                "timeout_usec": 5000,
                "header": "@",
                "terminator": "\n",
                "n_Bytes": 14
            }
        }
    },
    "deviceIO": {
        "esp32_fsr0": {
            "type": "SensorIO",
            "config": {
                "source": {
                    "commIO": "serial0",
                    "lsb_byte": 1,
                    "n_bytes": 4,
                    "ctype": "float"
                },
                "eval": "abs(x*1.0)"
            }
        },
        "log0": {
            "type": "LogPrint",
            "config": {}
        },
        "fes0": {
            "type": "ScienceMode3",
            "config": {
                "iface": "/dev/ttyUSB1"
            }
        },
        "fes0_r": {
            "type": "Channel_sm3",
            "config": {
                "device": "fes0",
                "channel": 0,
                "mA": 8,
                "pw": 200,
                "Hz": 60,
                "ramp": 0,
                "max_mA": 8,
                "max_pw": 250,
                "mode": "mA"
            }
        }
    },
    "controlIO": {
        "fnc1": {
            "type": "Functional",
            "config": {
                "in": [
                    "esp32_fsr0"
                ],
                "out": [
                    "log0", "fes0_r"
                ],
                "eval": [
                    "y0 := x0;",
                    "y1 := x0/10;",
                    "y2 := 2*(1+sin(2*M_PI*.5*t));"
                ],
                "ts": "500ms",
                "wakeup": "0ms",
                "thread": "_thr_control0"
            }
        }
    },
    "log": {
        "enable": false,
        "path": "logs/"
    },
    "robot": {
        "name": "ExoTAO",
        "type": "biped",
        "sensors": [
            "enc_epos5"
        ],
        "actuators": [
            "motor_epos5"
        ],
        "controllers": [
            "SimplePD"
        ]
    },
    "core": {
        "max_run_time": "5s"
    }
}